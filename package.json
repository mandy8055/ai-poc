{
  "name": "ai-poc",
  "version": "1.0.0",
  "type": "module",
  "main": "server.js",
  "description": "AI-powered product recommendation engine with mock data generation",
  "scripts": {
    "build": "tsc --project config/tsconfig.json",
    "codegen": "graphql-codegen --config config/codegen.ts",
    "codegen:watch": "graphql-codegen --config config/codegen.ts --watch",
    "generate:washers": "pnpm run build && node build/utils/generate-one-product.js 1 20",
    "test:washing": "python src/core/recommend_similar_products.py src/mocks/washing_machines/data.json 4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@graphql-codegen/typescript-resolvers": "^4.5.1",
    "@types/node": "^24.3.3",
    "graphql": "^16.11.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {},
  "packageManager": "pnpm@10.12.2+sha512.a32540185b964ee30bb4e979e405adc6af59226b438ee4cc19f9e8773667a66d302f5bfee60a39d3cac69e35e4b96e708a71dd002b7e9359c4112a1722ac323f",
  "keywords": [],
